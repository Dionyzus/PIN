{% extends 'base.html.twig' %}

{% block body_id 'subject_editStatus' %}

{% block main %}
    <h1>{{ 'Edit subject status' }}</h1>

    <div class="alert alert-info" role="alert">{{ 'Edit Status' }}</div>
    {{ form_start(form) }}
    {{ form_widget(form) }}

    <button type="submit" class="btn btn-primary">
        <i class="fa fa-save" aria-hidden="true"></i> {{ 'action.save'|trans }}
    </button>
    {{ form_end(form) }}
{% endblock %}

