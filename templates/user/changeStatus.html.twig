{% extends 'base.html.twig' %}

{% block body_id 'subject_editStatus' %}

{% block main %}
    <div class="alert alert-info" role="alert">{{ 'Edit status for subject :' }} {{ studentEnrolledSubject.subject }} </div>
    {{ form_start(form) }}
    {{ form_widget(form) }}

    <button type="submit" class="btn btn-primary">
        <i class="fa fa-save" aria-hidden="true"></i> {{ 'action.save'|trans }}
    </button>
    <a href="{{ path('user_editSubjects') }}" class="btn btn-link">
        <i class="fa fa-list-alt" aria-hidden="true"></i> {{ 'Back to List' }}
    </a>
    {{ form_end(form) }}
{% endblock %}

